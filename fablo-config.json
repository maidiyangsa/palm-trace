{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.1.0/schema.json",
  "global": {
    "fabricVersion": "2.4.3",
    "tls": false,
    "peerDevMode": true,
    "tools": {
      "explorer": true
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "PalmSafeOrderer",
        "domain": "orderer.palmsafe.com"
      },
      "orderers": [
        {
          "groupName": "palmsafe-orderer-group",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "Petani",
        "domain": "petani.palmsafe.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "Koperasi",
        "domain": "koperasi.palmsafe.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "PabrikKelapaSawit",
        "domain": "pks.palmsafe.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "Dinas",
        "domain": "dinas.palmsafe.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    }
  ],
  "channels": [
    {
      "name": "referensi-harga-channel",
